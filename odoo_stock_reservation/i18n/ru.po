"                           <td valign=\"center\">\n"
"                                ${stock_res['name']}\n"
"                           </td>\n"
"                           <td valign=\"center\" >\n"
"                                ${stock_res['product_id'].name}\n"
"                           </td>\n"
"                           <td align=\"right\">\n"
"                                ${stock_res['reserved_qty']}\n"
"                           </td>\n"
